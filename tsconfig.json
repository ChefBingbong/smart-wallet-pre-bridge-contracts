{
	"display": "Default",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"inlineSources": false,
		"isolatedModules": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"preserveWatchOutput": true,
		"skipLibCheck": true,
		"target": "es5",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist",
		"composite": true
	},
	"include": ["./test", "./src", "./utils", "typechain/**/*"],
	"files": ["./utils/env.ts"]
}
